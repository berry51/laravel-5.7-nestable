{
    "name": "berry51/laravel-nestable",
    "type": "library",
    "description": "Laravel 5.7 nested categories library",
    "homepage": "https://github.com/berry51/nestable",
    "keywords": ["nested", "nestable", "nestedset", "category", "laravel"],
    "license": "MIT",
    "authors": [{
        "name": "Berry",
        "email": "gdlr51@gmail.com",
        "homepage": "http://www.icngo.cn",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "illuminate/database": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^7.0",
        "orchestra/testbench": "^3.7",
        "orchestra/database": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Nestable\\": "src/",
            "Nestable\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nestable\\NestableServiceProvider"
            ],
            "aliases": {
                "Nestable": "Nestable\\Facades\\NestableService"
            }
        }
    },
    "minimum-stability": "dev"
}